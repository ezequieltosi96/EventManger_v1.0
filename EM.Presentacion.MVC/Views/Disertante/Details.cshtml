@model EM.Presentacion.MVC.Models.Disertante.DisertanteViewModel
@{
    ViewData["Title"] = "Detalle Disertante";
    Layout = "_LayoutAdmin";
}

<h1 class="text-center">@ViewData["Title"]</h1>

<div class="row">
    <div class="col-md-4 col-md-offset-4">
        <dl class="dl-horizontal">
            <dt>@Html.DisplayNameFor(model => model.ApyNom)</dt>
            <dd>@Model.ApyNom</dd>
            <dt>@Html.DisplayNameFor(model => model.Dni)</dt>
            <dd>@Model.Dni</dd>
            <dt>@Html.DisplayNameFor(model => model.Empresa.RazonSocial)</dt>
            <dd>@Model.Empresa.RazonSocial</dd>
            <dt>@Html.DisplayNameFor(model => model.EstaEliminado)</dt>
            <dd>@Model.EstaEliminado</dd>
        </dl>
    </div>
</div>

<div>
    <a class="btn btn-default" asp-action="Index" asp-route-empresaId="@ViewBag.EmpresaId"><span class="glyphicon glyphicon-menu-left"></span>Volver a la lista</a>
    <a class="btn btn-primary" asp-action="Edit" asp-route-id="@Model.Id"><span class="glyphicon glyphicon-pencil"></span></a>
</div>